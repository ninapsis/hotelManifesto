[autoload]

Utils="*res://Utils.gd"
ResourceLoader0="*res://ResourceLoader.gd"
Events="*res://Events.gd"
ND="*res://NPCDialog.gd"
SaverAndLoader="*res://SaverAndLoader.gd"
SoundFx="*res://Music and Sounds/SoundFX.tscn"
